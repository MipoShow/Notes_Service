<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <artifactId>msf4j-service</artifactId>
    <groupId>org.wso2.msf4j</groupId>
    <version>2.7.7</version>
    <relativePath>../pom.xml/pom.xml</relativePath>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.mipo</groupId>
  <artifactId>polsourcenotesms</artifactId>
  <name>polsourcenotesms</name>
  <version>1.0-SNAPSHOT</version>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-assembly-plugin</artifactId>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>single</goal>
            </goals>
            <configuration>
              <archive>
                <manifest>
                  <mainClass>com.mipo.polsourcenotesms.Application</mainClass>
                </manifest>
              </archive>
              <descriptorRefs>
                <descriptorRef>jar-with-dependencies</descriptorRef>
              </descriptorRefs>
            </configuration>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <properties>
    <netty.version>4.0.30.Final</netty.version>
    <rs-api.version>2.0</rs-api.version>
    <slf4j.version>1.7.5</slf4j.version>
    <gson.version>2.2.4</gson.version>
    <hsqldb.version>2.2.8</hsqldb.version>
    <slf4j.log4j.version>1.6.0</slf4j.log4j.version>
    <hibernate.version>4.3.2.Final</hibernate.version>
    <spring.version>4.2.0.RELEASE</spring.version>
  </properties>
</project>

